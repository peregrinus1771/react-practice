{
    "name": "myblog",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "run-p dev:*",
        "dev:next": "next dev -p 3003",
        "dev:path": "pathpida --ignorePath .gitignore -o src/lib/pathpida -e src/lib/pathpida --watch",
        "build": "next build && pathpida --ignorePath .gitignore -o src/lib/pathpida -e src/lib/pathpida",
        "start": "next start",
        "fix:lint": "eslint . --ext .tsx,.ts",
        "fix:format": "prettier --write .",
        "fix": "run-p fix:*",
        "test": "jest",
        "test-all": "yarn lint && yarn test",
        "api:build": "aspida"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "yarn fix"
        ]
    },
    "dependencies": {
        "@aspida/fetch": "^1.7.1",
        "aspida": "^1.7.1",
        "cheerio": "^1.0.0-rc.10",
        "dayjs": "^1.10.7",
        "highlight.js": "^11.4.0",
        "microcms-js-sdk": "^2.0.0",
        "next": "12.0.10",
        "next-seo": "^5.1.0",
        "next-themes": "^0.0.15",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-scroll": "^1.8.4"
    },
    "devDependencies": {
        "@testing-library/react": "^12.1.2",
        "@types/node": "^17.0.17",
        "@types/react": "^17.0.39",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "autoprefixer": "^10.4.2",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-tailwindcss": "^3.4.3",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "npm-run-all": "^4.1.5",
        "pathpida": "^0.17.0",
        "postcss": "^8.4.6",
        "prettier": "^2.5.1",
        "react-icons": "^4.3.1",
        "react-is": "^17.0.1",
        "tailwindcss": "^3.0.20",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    }
}
