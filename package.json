{
    "name": "myblog",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3003",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . --ext .tsx,.ts",
        "format": "prettier --write .",
        "fix": "yarn lint&&yarn format",
        "test": "jest",
        "test-all": "yarn lint && yarn test",
        "api:build": "aspida"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "yarn fix"
        ]
    },
    "dependencies": {
        "dayjs": "^1.10.4",
        "next": "10.0.7",
        "next-seo": "^4.19.0",
        "react": "17.0.1",
        "react-dom": "17.0.1"
    },
    "devDependencies": {
        "@aspida/fetch": "^1.7.1",
        "@testing-library/react": "^11.2.5",
        "@types/node": "^14.14.28",
        "@types/react": "^17.0.2",
        "@types/styled-components": "^5.1.7",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "aspida": "^1.7.1",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^5.1.3",
        "lint-staged": "^10.5.4",
        "microcms-js-sdk": "^2.0.0",
        "prettier": "^2.2.1",
        "react-icons": "^4.2.0",
        "react-is": "^17.0.1",
        "styled-components": "^5.2.1",
        "ts-jest": "^26.5.3",
        "typescript": "^4.1.5"
    }
}
